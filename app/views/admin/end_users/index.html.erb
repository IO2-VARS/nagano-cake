<div class="container">
	<div class="row">
		<h1><strong>会員一覧画面</strong></h1><br>

		<div class="col-xs-10 col-xs-offset-2">
			<table class="table table-hover table-inverse">
				<thead>
					<tr>
						<th>会員ID</th>
						<th>氏名</th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>

				<tbody>
					<% @end_users.each do |end_user| %>
						<tr>
							<td><%= end_user.id %></td>
							<td><%= link_to end_user.family_name + end_user.first_name, admin_end_user_path(end_user) %></td>
							<td><%= end_user.email %></td>
							<td>
								<% if end_user.is_unsubscribed == false %>
									<p>有効</p>
								<% else %>
									<p>退会済</p>
								<% end %>
							</td>
						</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>