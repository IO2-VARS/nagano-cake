<div class="container">
	<div class="row">
		<div class="col-xs-3">
			<h1>会員一覧画面</h1>
		</div>

		<div class="col-xs-6">
			<table class="table table-hover table-inverse">
				<thead>
					<tr>
						<th>会員ID</th>
						<th>氏名</th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>

				<tbody>
					<% @end_users.each do |end_user| %>
						<tr>
							<td><%= end_user.id %></td>
							<td><%= link_to end_user.family_name + end_user.first_name, admin_end_user_path(end_user) %></td>
							<td><%= end_user.email %></td>
							<td><%= end_user.is_unsubscribed %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
		</div>

		<div class="col-xs-3"></div>

	</div>
</div>