<h3>新規会員登録</h3>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

<div class="field">
<%= f.label :"名前" %>
<p><span>(姓)</span> <%= f.text_field :family_name %>
<span>(名)</span> <%= f.text_field :first_name %></p>
</div>

<div class="field">
<%= f.label :"フリガナ" %>
<p><span>(セイ)</span> <%= f.text_field :family_name_kana %>
<span>(メイ)</span> <%= f.text_field :first_name_kana %></p>
</div>



<div class="field">
<%= f.label :"メールアドレス" %>
<p><%= f.email_field :email, autocomplete: "email" %></p>
</div>

<div class="field">
<%= f.label :"郵便番号" %>
<p><%= f.text_field :zip_code %></p>
</div>

<div class="field">
<%= f.label :"住所" %>
<p><%= f.text_field :address %></p>
</div>

<div class="field">
<%= f.label :"電話番号" %>
<p><%= f.text_field :phone_number %></p>
</div>

<div class="field">
<%= f.label :"パスワード(6文字以上)" %>
<p><%= f.password_field :password, autocomplete: "new-password" %></p>
</div>

<div class="field">
<%= f.label :"パスワード確認用" %>
<p><%= f.password_field :password_confirmation, autocomplete: "new-password" %></p>
</div>


<div class="actions">
<%= f.submit "新規登録",class:"btn btn-info" %>
</div>
<% end %>

<h3>既に登録済みの方</h3>
<%= link_to "こちら" ,new_end_user_session_path %>からログインしてください。




